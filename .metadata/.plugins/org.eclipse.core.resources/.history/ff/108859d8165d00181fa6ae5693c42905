
public class HelloWord {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		// primitive type
		int i  = 1_000_000; 
		i = 321;
		long l = 5_000_000_000l;
		float f = 1.0f;
		double d = -1.111_000;
		boolean b = true;
		char c = 'U';
		
		// objects
		
		
System.out.println("Hello Word!");
	}

}
